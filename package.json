{
  "name": "@ott/picturesque",
  "description": "Application for generating responsive background images",
  "type": "module",
  "private": true,
  "author": "One Two Trip",
  "repository": "onetwotrip/picturesque",
  "license": "MIT",
  "scripts": {
    "start": "netlify dev",
    "start:client": "vite --clearScreen",
    "build": "netlify build",
    "build:client": "vite build",
    "test:css": "stylelint \"**/*.{svelte,css}\"",
    "test:format": "prettier --check \"**/*.{js,ts,svelte,css,json,md,yml}\"",
    "test:js": "eslint **/*.{svelte,js,ts}",
    "test:size": "pnpm run build:client && size-limit",
    "test:spelling": "cspell \"**/*\"",
    "test:svelte": "svelte-check",
    "test:types": "tsc --noEmit --pretty",
    "test": "pnpm run /^test:/"
  },
  "dependencies": {
    "@netlify/functions": "^2.7.0",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.3",
    "browserslist": "^4.23.1",
    "busboy": "^1.6.0",
    "jimp": "^0.22.12",
    "jszip": "^3.10.1",
    "lightningcss": "^1.25.1",
    "nanostores": "^0.10.3",
    "sharp": "^0.33.4",
    "shiki": "^1.6.4",
    "shiki-magic-move": "^0.4.2",
    "svelte": "5.0.0-next.155",
    "svelte-preprocess": "^6.0.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-html": "^3.2.2"
  },
  "devDependencies": {
    "@azat-io/eslint-config-typescript": "^1.9.0",
    "@azat-io/stylelint-config": "^0.1.1",
